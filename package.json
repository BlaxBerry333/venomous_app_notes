  "description": "blaxberry333's venomous apps notes",
  "type": "module",
  "scripts": {
    "start:dev": "nuxt dev --dotenv .env.development",
    "start:prod": "node .output/server/index.mjs",
    "build:dev": "nuxt build --dotenv .env.development",
    "build:prod": "nuxt build --dotenv .env.production",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "commit": "cd .. && npm run commit",
    "check-packages": "npx depcheck",
